{
    "id": {
      "label": "Building ID",
      "type": "identifier",
      "visible": true,
      "searchable": true
    },
    "name": {
      "label": "Building Name", 
      "type": "text",
      "visible": true,
      "searchable": true
    },
    "squareMeters": {
      "label": "Area",
      "type": "numeric",
      "unit": "m²",
      "decimals": 0,
      "visible": true,
      "colorMode": "gradient",
      "colors": {
        "min": {"r": 200, "g": 230, "b": 255},
        "max": {"r": 30, "g": 100, "b": 200}
      }
    },
    "temperature": {
      "label": "Temperature",
      "type": "numeric", 
      "unit": "°C",
      "decimals": 1,
      "visible": true,
      "colorMode": "bins",
      "bins": [
        {"min": 0, "max": 10, "label": "Cold (<10°C)", "color": "#3B82F6", "borderColor": "#1D4ED8"},
        {"min": 10, "max": 18, "label": "Cool (10-18°C)", "color": "#06B6D4", "borderColor": "#0891B2"},
        {"min": 18, "max": 25, "label": "Comfort (18-25°C)", "color": "#10B981", "borderColor": "#059669"},
        {"min": 25, "max": 30, "label": "Warm (25-30°C)", "color": "#F59E0B", "borderColor": "#D97706"},
        {"min": 30, "max": 100, "label": "Hot (>30°C)", "color": "#EF4444", "borderColor": "#DC2626"}
      ]
    },
    "client": {
      "label": "Client",
      "type": "categorical",
